{
  "name": "solidapi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production babel src --extensions \".js,.ts\" --out-dir dist --copy-files -no-copy-ignored",
    "start:dev": "NODE_ENV=dev tsnd -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules --no-notify src/server.ts",
    "seed:dev": "NODE_ENV=dev tsnd seeds/index.ts",
    "start:local": "NODE_ENV=local tsnd -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules --no-notify src/server.ts",
    "seed:local": "NODE_ENV=local tsnd seeds/index.ts",
    "docker-up": "docker-compose --env-file .env.dev up",
    "docker-build": "docker-compose --env-file .env.dev build"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.5",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.3",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
